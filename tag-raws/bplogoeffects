{sh:load {video}
vidlen=$(ffprobe -i $FILE_1 -show_entries format=duration -v quiet -of csv="p=0")
ffmpeg -i $FILE_1 -b:v 2500k 1.ts
ffmpeg -i $FILE_1 -vf huesaturation=-60:0.25:-0.025:-100:100 -filter_complex "[0:a]rubberband=formant=712923000:transients=crisp:detector=percussive:smoothing=2.14748e+09/4.9:pitchq=quality:pitch=2^(-1/12)[a1];[a1]amix=1,volume=1,atrim=0.02[outa]" -map 0:v -map "[outa]" -b:v 2500k -t $vidlen 2.ts
ffmpeg -i $FILE_1 -vf huesaturation=180:0.25:-0.025:-100:100,negate -filter_complex "[0:a]rubberband=formant=712923000:transients=crisp:detector=percussive:smoothing=2.14748e+09/4.9:pitchq=quality:pitch=2^(8/12)[a1];[a1]amix=1,volume=1,atrim=0.02[outa]" -map 0:v -map "[outa]" -b:v 2500k -t $vidlen 3.ts
ffmpeg -i $FILE_1 -vf negate -filter_complex "[0:a]rubberband=formant=712923000:transients=crisp:detector=percussive:smoothing=2.14748e+09/4.9:pitchq=quality:pitch=2^(0/12)[a1];[0:a]rubberband=formant=712923000:transients=crisp:detector=percussive:smoothing=2.14748e+09/4.9:pitchq=quality:pitch=2^(3/12)[a2];[a1][a2]amix=2,volume=2,atrim=0.02[outa]" -map 0:v -map "[outa]" -b:v 2500k -t $vidlen 4.ts
ffmpeg -i $FILE_1 -vf huesaturation=-140:0.25:-0.025:-100:100,format=yuv444p,transpose=1,"geq='p(X,Y+sin(tan(sin(cos(abs(T*-100+40/10+X*0.09)))))*3)'","geq='p(X,Y+sin(tan(sin(cos(abs(T*200/10+X*0.08)))))*2.5)'","geq='p(X,Y+sin(abs(tan(sin(tan(T*-20+360/10+X*0.02)))))*4)'","geq='p(X,Y+sin(tan(sin(T*-48+420/10+X*0.075)))*3.6)'","geq='p(X,Y+sin(tan(sin(cos(abs(T*40+690/10+X*0.1)))))*4)'",scale=iw:ih,transpose=2,format=yuv420p -filter_complex "[0:a]rubberband=formant=712923000:transients=crisp:detector=percussive:smoothing=2.14748e+09/4.9:pitchq=quality:pitch=2^(0/12)[a1];[0:a]rubberband=formant=712923000:transients=crisp:detector=percussive:smoothing=2.14748e+09/4.9:pitchq=quality:pitch=2^(2/12)[a2];[a1][a2]amix=2,volume=2,atrim=0.02[outa]" -map 0:v -map "[outa]" -b:v 2500k -t $vidlen 5.ts
ffmpeg -i "concat:1.ts|2.ts|3.ts|4.ts|5.ts" ./output/bplogoeffects.mov}